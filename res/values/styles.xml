<resources>
    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->

    <style name="AppBaseTheme1" parent="android:Theme.Holo.Light"/>
    <style name="AppTheme1" parent="AppBaseTheme"/>

    <style name="AppBaseTheme" parent="@android:style/Theme.Light"/>
    <style name="AppTheme" parent="@style/AppBaseTheme" />

    <style name='ETTheme' parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:textColor">#ffffff</item>
        <item name="android:divider">#ffd12a2e</item>
        <item name="android:itemBackground">@color/color_app_theme</item>
        <item name="android:actionBarStyle">@style/ActionBarCustom</item>
        <item name="android:actionBarTabStyle">@style/TabCustom</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:colorBackground">#ccf8fc13</item>
        <item name="android:homeAsUpIndicator">@drawable/up_arrow</item>
    </style>

    <style name='MenuTheme' parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:textColor">#000000</item>
        <item name="android:actionBarStyle">@style/ActionBarCustomMenu</item>
        <item name="android:itemBackground">@color/color_app_theme</item>
        <item name="android:colorBackground">#ccf8fc13</item>
        <item name="android:gravity">start</item>
        <item name="android:homeAsUpIndicator">@drawable/up_arrow</item>
    </style>

    <style name="ActionBarCustom" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse" >
        <item name="android:icon">@drawable/company_logo</item>
        <item name="android:text">@string/app_blank_name</item>
        <item name="android:textSize">3sp</item>
        <item name="android:background">@color/color_app_theme</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:displayOptions">homeAsUp|showHome|showTitle</item>
        <item name="android:titleTextStyle">@style/TitleBarTextColor</item>
        <item name="android:windowActionBarOverlay">true</item>
    </style>

    <style name="ActionBarCustomMenu" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse" >
        <item name="android:icon">@drawable/company_logo</item>
        <item name="android:text">@string/app_blank_name</item>
        <item name="android:textSize">3sp</item>
        <item name="android:background">@color/color_app_theme</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:displayOptions">homeAsUp|showHome|showTitle</item>
        <item name="android:titleTextStyle">@style/TitleBarTextColor</item>
        <item name="android:windowActionBarOverlay">true</item>
    </style>

    <style parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse" name="TabCustom">
        <item name="android:background">@drawable/tabbar</item>
    </style>

    <style parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse" name="TabBarCustom">
        <item name="android:background">@drawable/tabbar</item>
    </style>

    <style parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title" name="TitleBarTextColor">
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="layoutGen_250">
        <item name="android:layout_height">300dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:background">@color/background3</item>
    </style>

    <style name="layoutTrans_150">
        <item name="android:layout_height">150dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/background1</item>
    </style>

    <style name="layoutTrans_250">
        <item name="android:layout_height">250dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/background1</item>
    </style>

    <style name="block_title">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">15sp</item>
    </style>
    
    <style name="layout_day">
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">30dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_margin">1dp</item>
        <item name="android:padding">1dp</item>
    </style>

    <style name="tv_week">
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">1dp</item>
        <item name="android:textSize">10sp</item>
    </style>
    
    <style name="tv_time">
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">1dp</item>
        <item name="android:textSize">8sp</item>
    </style>


    <style name="app_font">
        <item name="android:textColor">@color/color_app_text_w</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">normal</item>
    <item name="android:typeface">sans</item>
    </style>


    <style name="rowStyle2">
        <item name="android:layout_width">25dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">14sp</item>
        <item name="android:padding">2dp</item>
        <item name="android:gravity">start</item>
    </style>

    <!-- Layout Themes -->
    <style parent="android:Theme" name="ETTheme1">
        <item name="android:textColor">#ff2a8b02</item>
        <item name="android:divider">#ffd1d1d1</item>
        <item name="android:itemBackground">#fff1f2f2</item>
        <item name="android:background">#ff2a8b02</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style parent="@android:style/Theme.Holo.Light.DarkActionBar" name="ETTheme2">
        <item name="android:textColor">#ff73d1d0</item>
        <item name="android:divider">#ff73d1d0</item>
        <item name="android:itemBackground">#fff1f2f2</item>
    </style>

    <style parent="@android:style/Theme.Holo.Light.DarkActionBar" name="ETTheme3">
        <item name="android:textColor">#ff1f6d04</item>
        <item name="android:divider">#ff50d12b</item>
        <item name="android:itemBackground">#fff1f2f2</item>
    </style>
</resources>